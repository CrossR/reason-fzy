{
  "name": "reason-fzy",
  "version": "1.0",
  "description": "Reason bindings for fzy",
  "license": "MIT",
  "scripts": {
    "format": "esy dune build @fmt --auto-promote"
  },
  "esy": {
    "build": ["refmterr dune build -p Fzy"],
    "buildsInSource": "_build",
    "install": [
        "esy-installer Fzy.install"
    ]
  },
  "dependencies": {
    "@opam/dune": "*",
    "@opam/dune-configurator": "*",
    "@esy-ocaml/reason": "^3.4.0",
    "refmterr": "*",
    "ocaml": ">=4.7.0 <4.10.0",
    "esy-fzy": "github:CrossR/esy-fzy#e1bbbf1"
  },
  "resolutions": {
    "@opam/ocaml-migrate-parsetree": "1.5.0",
    "@opam/easy-format": "1.3.1"
  },
  "devDependencies": {
    "ocaml": "~4.9.0"
  }
}
