    "install": [
        "esy-installer Fzy.install"
    ]
  },
  "dependencies": {
    "@opam/dune": ">=1.6.0",
    "@esy-ocaml/reason": "^3.4.0",
    "@reason-native/rely": "^1.3.1",
    "refmterr": "^3.2.2",
    "ocaml": ">=4.6.0",
    "esy-fzy": "*",
    "reperf": "^1.3.0"
  },
  "resolutions": {
    "@opam/easy-format": "1.3.1",
    "esy-fzy": "github:CrossR/esy-fzy#5ca1db1"
  },
  "devDependencies": {
    "ocaml": "4.8.1000"
  }
}
