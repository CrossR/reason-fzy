{
  "name": "reason-fzy",
  "version": "1.0",
  "description": "Reason bindings for fzy",
  "license": "MIT",
  "scripts": {
    "format": "esy dune build @fmt --auto-promote"
  },
  "esy": {
    "build": ["refmterr dune build -p Fzy"],
    "buildsInSource": "_build",
    "install": [
        "esy-installer Fzy.install"
    ]
  },
  "dependencies": {
    "@opam/dune": "*",
    "@opam/dune-configurator": "*",
    "@opam/reason": "^3.6.0",
    "refmterr": "*",
    "ocaml": ">=4.7.0",
    "esy-fzy": "github:CrossR/esy-fzy#c42c543"
  },
  "devDependencies": {
    "ocaml": "~4.9.0"
  }
}
