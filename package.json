{
  "name": "reason-fzy",
  "version": "1.0",
  "description": "Reason bindings for fzy",
  "license": "MIT",
  "scripts": {
    "format": "esy dune build @fmt --auto-promote"
  },
  "esy": {
    "build": ["refmterr dune build -p Fzy"],
