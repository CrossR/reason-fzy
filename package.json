  "dependencies": {
    "@opam/dune": "*",
    "@opam/dune-configurator": "*",
    "@opam/ppx_inline_test": "v0.14.1",
    "@opam/reason": "^3.6.0",
    "refmterr": "*",
    "ocaml": ">=4.7.0",
    "esy-fzy": "github:bryphe/esy-fzy#301dbf6"
  },
  "devDependencies": {
    "ocaml": "~4.9.0"
  }
}
